{
  "name": "expresstemplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "init-db": "ts-node ./scripts/run-init-db.ts",
    "serve": "npm run build && concurrently --kill-others \"npm run build-watch\" \"npm run serve-debug\"",
    "serve-init-db": "npm run build && npm run init-db && concurrently --kill-others \"npm run build-watch\" \"npm run serve-debug\"",
    "build": "tsc",
    "build-watch": "tsc -w",
    "serve-debug": "nodemon --inspect dist/server.js",
    "lint": "tslint --project .",
    "coverage": "npm run init-db && jest --forceExit --detectOpenHandles --coverage",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongodb": "^3.1.30",
    "@types/supertest": "^2.0.8",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.0-beta2",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
